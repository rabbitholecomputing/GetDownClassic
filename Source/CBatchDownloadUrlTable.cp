#include "CBatchDownloadUrlTable.h"void CBatchDownloadUrlTable::DrawCell(const TableCellT &inCell){	Rect frame;	int x = inCell.col;	int y = inCell.row;		BatchURL batchUrl;	GetCellData((TableCellT){ y, 1 }, &batchUrl);		if(x == 1 && batchUrl.url[0] > 0)	{		FetchLocalCellFrame(inCell, frame);		::MacFrameRect(&frame);		::MoveTo(frame.left + 4, frame.bottom - 6);				Str255 tmpString;		LStr255::CopyPStr(batchUrl.url, tmpString);		::TruncString(frame.right - frame.left - 8, tmpString, truncEnd); 		::DrawString(tmpString);	}	if(x == 2 && batchUrl.url[0] > 0)	{		FetchLocalCellFrame(inCell, frame);		::MacFrameRect(&frame);		::MoveTo(frame.left + 4, frame.bottom - 6);				::DrawString(LStr255(strN_BatchStatus, batchUrl.status));	}}CBatchDownloadUrlTable::CBatchDownloadUrlTable(LStream *inStream) : LTable(inStream){}