#include "CUrlImportDialogHandler.h"#include "Constants.h"void CUrlImportDialogHandler::ListenToMessage(	MessageT	inMessage,	void*		ioParam){	mMessage = inMessage;		// Store message. DoDialog() will return								//   this value.	inParam   = ioParam;		if(inMessage == msg_DLDataAvailable)	{		bBatchDataAvailable = true;		hBatchData = (Handle)ioParam;	}	else if(inMessage == msg_DLSuccess)	{		bDLSuccess = true;	}	else if(inMessage == msg_DLFailure)	{		bDLFailure = true;		((LStdButton *)this->GetDialog()->FindPaneByID(1))->Enable();	}	else if(inMessage == 9999)	{		((LStdButton *)this->GetDialog()->FindPaneByID(1))->Enable();	}}void CUrlImportDialogHandler::ResetSemaphores(){	bBatchDataAvailable = false;	bDLSuccess = false;	bDLFailure = false;}CUrlImportDialogHandler::CUrlImportDialogHandler(ResIDT inDialogResID, LCommander *inSuper) 	: StDialogHandler(inDialogResID, inSuper){}